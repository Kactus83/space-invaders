{
    "compilerOptions": {
      "outDir": "./dist",
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "lib": ["ES2015", "DOM"],
    },
    "include": [
      "./src/**/*"
    ]
  }
  